    "get": {
        "tags": [
            "Posts"
        ],
        "summary": "Get All Post Previews",
        "description": "Get All Post Previews",
        "operationId": "post.get-all-previews",
        "parameters": [
            {
                "name": "limit",
                "in": "query",
                "description": "Maximum tag count (Default: 10)",
                "schema": {
                    "type": "number"
                }
            },
            {
                "name": "page",
                "in": "query",
                "description": "Page number (Default: 1)",
                "schema": {
                    "type": "number"
                }
            },
            {
                "in": "query",
                "name": "props",
                "schema": {
                    "type": "object",
                    "properties": {
                        "title.eq": {
                            "type": "string",
                            "example": "Bordo Blog"
                        },
                        "createdAt.eqd": {
                            "type": "string",
                            "example": "2023  or  2023-03  or  2023-03-12  or  2023-03-12T18  or  2023-03-12T18:20  or  2023-03-12T18:20:45"
                        },
                        "createdAt.gt": {
                            "type": "string",
                            "example": "1662361877178"
                        },
                        "createdAt.lt": {
                            "type": "string",
                            "example": "1662361877178"
                        },
                        "createdAt.gte": {
                            "type": "string",
                            "example": "1662361877178"
                        },
                        "createdAt.lte": {
                            "type": "string",
                            "example": "1662361877178"
                        },
                        "tags.in": {
                            "type": "string",
                            "example": "movie, software"
                        }
                    }
                },
                "style": "form",
                "explode": true
            },
            {
                "name": "sortField",
